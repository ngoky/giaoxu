{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@svg/*": [
                "src/assets/svg/*"
            ],
            "@img/*": [
                "src/assets/images/*"
            ],
            "@icons/*": [
                "src/assets/icons/*"
            ],
            "@shared/*": [
                "src/shared/*"
            ],
            "@/components/*": [
                "src/components/*"
            ],
            "@/constants/*": [
                "src/constants/*"
            ],
            "@/pages/*": [
                "src/pages/*"
            ],
            "@/admin/*": [
                "src/admin/*"
            ],
        }
    }
}