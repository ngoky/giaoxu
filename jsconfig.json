{
    "extends": "./jsconfig.path.json",
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@/components/*": [
                "./components/*"
            ],
            "@/pages/*": [
                "./pages/*"
            ],
            "@/admin/*": [
                "./admin/*"
            ],
            "@/constants/*": [
                "./constants/*"
            ],
            "@/utils/*": [
                "./utils/*"
            ]
        }
    },
    "include": [
        "./src"
    ],
    "exclude": [
        "./node_modules"
    ]
}